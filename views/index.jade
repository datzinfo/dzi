doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
    title DatzInfo : Home
    // Favicon
    link(rel='shortcut icon', type='image/icon', href='images/favicon.ico')
    // Font Awesome
    link(href='css/font-awesome.css', rel='stylesheet')
    // Bootstrap
    link(href='css/bootstrap.css', rel='stylesheet')
    // Slick slider
    link(rel='stylesheet', type='text/css', href='css/slick.css')
    // Fancybox slider
    link(rel='stylesheet', href='css/jquery.fancybox.css', type='text/css', media='screen')
    // Animate css
    link(rel='stylesheet', type='text/css', href='css/animate.css')
    // Progress bar
    link(rel='stylesheet', type='text/css', href='css/bootstrap-progressbar-3.3.4.css')
    // Theme color
    link#switcher(href='css/theme-color/datzInfo_theme.css', rel='stylesheet')
    // Main Style
    link(href='js/style.css', rel='stylesheet')
    // Fonts
    // Open Sans for body font
    link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet', type='text/css')
    // Lato for Title
    link(href='https://fonts.googleapis.com/css?family=Lato', rel='stylesheet', type='text/css')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body
    // BEGAIN PRELOADER
    #preloader
      #status  
    // END PRELOADER
    // SCROLL TOP BUTTON
    a.scrollToTop(href='#')
      i.fa.fa-angle-up
    // END SCROLL TOP BUTTON
    // BEGIN MENU
    section#menu-area
      nav.navbar.navbar-default(role='navigation')
        .container
          .navbar-header
            // FOR MOBILE VIEW COLLAPSED BUTTON
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            // LOGO
            a.navbar-brand(href='index.html')
              img(src='images/datzinfo_logo.png', alt='DataInfo')
          #navbar.navbar-collapse.collapse
            ul#top-menu.nav.navbar-nav.navbar-right.main-nav
              li.active
                a(href='index.html') Home
              li
                a(href='aboutUs.html') About Us
              li.dropdown
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  | Services 
                  span.fa.fa-angle-down
                ul.dropdown-menu(role='menu')
                  li
                    a(href='service.html') Service 1
                  li
                    a(href='@') Service 2
                  li
                    a(href='@') Service 3
                  li
                    a(href='@') Service 4
              li
                a(href='blog.html') Blog
              li
                a(href='contact.html') Contact
          // /.nav-collapse
          a#search-icon(href='#')
            i.fa.fa-search
    // END MENU
    // Start slider
    section#slider
      .main-slider
        .single-slide
          img(src='images/slider-3.jpg', alt='img')
          .slide-content
            .container
              .row
                .col-md-6.col-sm-6
                  .slide-article
                    h1.wow.fadeInUp(data-wow-duration='0.5s', data-wow-delay='0.5s') #{ messages.index_slider1_title1 }
                    p.wow.fadeInUp(data-wow-duration='0.5s', data-wow-delay='0.75s') #{ messages.index_slider1_title2 }
                    a.read-more-btn.wow.fadeInUp(data-wow-duration='1s', data-wow-delay='1s', href='#') Read More
                .col-md-6.col-sm-6
                  .slider-img.wow.fadeInUp
                    img(src='images/person1.png', alt='business man')
        .single-slide
          img(src='images/slider-2.jpg', alt='img')
          .slide-content
            .container
              .row
                .col-md-6.col-sm-6
                  .slide-article
                    h1.wow.fadeInUp(data-wow-duration='0.5s', data-wow-delay='0.5s') #{ messages.index_slider2_title1 }
                    p.wow.fadeInUp(data-wow-duration='0.5s', data-wow-delay='0.75s') #{ messages.index_slider2_title2 }
                    a.read-more-btn.wow.fadeInUp(data-wow-duration='1s', data-wow-delay='1s', href='#') Read More
                .col-md-6.col-sm-6
                  .slider-img.wow.fadeInUp
                    img(src='images/person2.png', alt='business man')
    // End slider
    // Start Feature
    section#feature
      .container
        .row
          .col-md-12
            .title-area
              h2.title #{ messages.index_feature_title }
              span.line
              p #{ messages.index_feature_description }
          .col-md-12
            .feature-content
              .row
                .col-md-4.col-sm-6
                  .single-feature.wow.zoomIn
                    i.fa.fa-leaf.feature-icon
                    h4.feat-title #{ messages.index_box1_title }
                    p #{ messages.index_box1_desc }
                .col-md-4.col-sm-6
                  .single-feature.wow.zoomIn
                    i.fa.fa-mobile.feature-icon
                    h4.feat-title #{ messages.index_box2_title }
                    p #{ messages.index_box2_desc }
                .col-md-4.col-sm-6
                  .single-feature.wow.zoomIn
                    i.fa.fa-thumbs-o-up.feature-icon
                    h4.feat-title #{ messages.index_box3_title }
                    p #{ messages.index_box3_desc}
                .col-md-4.col-sm-6
                  .single-feature.wow.zoomIn
                    i.fa.fa-gears.feature-icon
                    h4.feat-title #{ messages.index_box4_title }
                    p #{ messages.index_box4_desc }
                .col-md-4.col-sm-6
                  .single-feature.wow.zoomIn
                    i.fa.fa-code.feature-icon
                    h4.feat-title #{ messages.index_box5_title }
                    p #{ messages.index_box5_desc }
                .col-md-4.col-sm-6
                  .single-feature.wow.zoomIn
                    i.fa.fa-smile-o.feature-icon
                    h4.feat-title #{ messages.index_box6_title }
                    p #{ messages.index_box6_desc }
    // End Feature
    // Start Testimonial section
    section#testimonial
      .container
        .row
          .col-md-6
            .row
              .col-md-12
                .title-area
                  h2.title Whats Client Say
                  span.line
              .col-md-12
                // Start testimonial slider
                .testimonial-slider
                  // Start single slider
                  .single-slider
                    .testimonial-img
                      img(src='images/testi1.jpg', alt='testimonial image')
                    .testimonial-content
                      p
                        | There are many variations of passages of Lorem Ipsum available, but the majority have suffered alteration in some form, by injected humour, or randomised words which don't look even slightly believable.
                      h6
                        | Bernice Neumann, 
                        span Designer
                  // Start single slider
                  .single-slider
                    .testimonial-img
                      img(src='images/testi3.jpg', alt='testimonial image')
                    .testimonial-content
                      p
                        | There are many variations of passages of Lorem Ipsum available, but the majority have suffered alteration in some form, by injected humour, or randomised words which don't look even slightly believable.
                      h6
                        | John Dow, 
                        span CEO
                  // Start single slider
                  .single-slider
                    .testimonial-img
                      img(src='images/testi2.jpg', alt='testimonial image')
                    .testimonial-content
                      p
                        | There are many variations of passages of Lorem Ipsum available, but the majority have suffered alteration in some form, by injected humour, or randomised words which don't look even slightly believable.
                      h6
                        | Michel, 
                        span Developer
          .col-md-6
    // End Testimonial section
    // Start Clients brand
    section#clients-brand
      .container
        .row
          .col-md-12
            .clients-brand-area
              ul.clients-brand-slide
                li.col-md-3
                  .single-brand
                    img(src='images/amazon.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/discovery.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/envato.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/tuenti.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/amazon.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/discovery.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/envato.png', alt='img')
                li.col-md-3
                  .single-brand
                    img(src='images/tuenti.png', alt='img')
    // End Clients brand
    // Start subscribe us
    section#subscribe
      .subscribe-overlay
        .container
          .row
            .col-md-12
              .subscribe-area
                h2.wow.fadeInUp Subscribe Newsletter
                form.subscrib-form.wow.fadeInUp(action='', data-wow-duration='0.5s', data-wow-delay='0.5s')
                  input(type='text', placeholder='Enter Your E-mail..')
                  button.subscribe-btn(type='submit') Submit
    // End subscribe us
    // Start footer
    footer#footer
      .container
        .row
          .col-md-6.col-sm-6
            .footer-left
              p
                | Designed by 
                a(href='http://www.markups.io/') MarkUps.io
          .col-md-6.col-sm-6
            .footer-right
              a(href='index.html')
                i.fa.fa-facebook
              a(href='#')
                i.fa.fa-twitter
              a(href='#')
                i.fa.fa-google-plus
              a(href='#')
                i.fa.fa-linkedin
              a(href='#')
                i.fa.fa-pinterest
    // End footer
    // jQuery library
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
    // Include all compiled plugins (below), or include individual files as needed
    // Bootstrap
    script(src='js/bootstrap.js')
    // Slick Slider
    script(type='text/javascript', src='js/slick.js')
    // mixit slider
    script(type='text/javascript', src='js/jquery.mixitup.js')
    // Add fancyBox
    script(type='text/javascript', src='js/jquery.fancybox.pack.js')
    // counter
    script(src='js/waypoints.js')
    script(src='js/jquery.counterup.js')
    // Wow animation
    script(type='text/javascript', src='js/wow.js')
    // progress bar
    script(type='text/javascript', src='js/bootstrap-progressbar.js')
    // Custom js
    script(type='text/javascript', src='js/custom.js')
