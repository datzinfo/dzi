doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
    title DatzInfo : Contact
    // Favicon
    link(rel='shortcut icon', type='image/icon', href='images/favicon.ico')
    // Font Awesome
    link(href='css/font-awesome.css', rel='stylesheet')
    // Bootstrap
    link(href='css/bootstrap.css', rel='stylesheet')
    // Slick slider
    link(rel='stylesheet', type='text/css', href='css/slick.css')
    // Fancybox slider
    link(rel='stylesheet', href='css/jquery.fancybox.css', type='text/css', media='screen')
    // Animate css
    link(rel='stylesheet', type='text/css', href='css/animate.css')
    // Bootstrap progressbar
    link(rel='stylesheet', type='text/css', href='css/bootstrap-progressbar-3.3.4.css')
    // Theme color
    link#switcher(href='css/theme-color/default-theme.css', rel='stylesheet')
    // Main Style
    link(href='css/style.css', rel='stylesheet')
    // Fonts
    // Open Sans for body font
    link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet', type='text/css')
    // Lato for Title
    link(href='https://fonts.googleapis.com/css?family=Lato', rel='stylesheet', type='text/css')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body
    // BEGAIN PRELOADER
    #preloader
      #status Â 
    // END PRELOADER
    // SCROLL TOP BUTTON
    a.scrollToTop(href='#')
      i.fa.fa-angle-up
    // END SCROLL TOP BUTTON
    // BEGIN MENU
    section#menu-area
      nav.navbar.navbar-default(role='navigation')
        .container
          .navbar-header
            // FOR MOBILE VIEW COLLAPSED BUTTON
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            // LOGO
            // TEXT BASED LOGO
            a.navbar-brand(href='index') DatzInfo
            // IMG BASED LOGO
            // <a class="navbar-brand" href="index"><img src="images/logo.png" alt="logo"></a>
          #navbar.navbar-collapse.collapse
            ul#top-menu.nav.navbar-nav.navbar-right.main-nav
              li
                a(href='index') Home
              li
                a(href='aboutUs') About Us
              li.dropdown
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  | Services 
                  span.fa.fa-angle-down
                ul.dropdown-menu(role='menu')
                  li
                    a(href='service') Service 1
                  li
                    a(href='@') Service 2
                  li
                    a(href='@') Service 3
                  li
                    a(href='@') Service 4
              li
                a(href='blog') Blog
              li.active
                a(href='contact') Contact
          // /.nav-collapse
    // END MENU
    // Start single page header
    section#single-page-header
      .overlay
        .container
          .row
            .col-md-6.col-sm-6.col-xs-12
              .single-page-header-left
                h2 #{ messages.contact_banner_title }
                p #{ messages.contact_banner_desc }
    // End single page header
    // Start contact section
    section#contact
      .container
        .row
          .col-md-12
            .title-area
              h2.title #{ messages.contact_mainquestions_title }
              span.line
              p #{ messages.contact_mainquestions_desc }
          .col-md-12
            .cotact-area
              .row
                .col-md-4
                  .contact-area-left
                    h4 #{ messages.contact_info_title }
                    p #{ messages.contact_info_desc }
                    address.single-address
                      p P.O. Box 240000. Huntsville, AL 35813, USA Street address: 305
                      p support@gmail.com
                      p +1(06) 778 5468
                    .footer-right.contact-social
                      a(href='index')
                        i.fa.fa-facebook
                      a(href='#')
                        i.fa.fa-twitter
                      a(href='#')
                        i.fa.fa-google-plus
                      a(href='#')
                        i.fa.fa-linkedin
                      a(href='#')
                        i.fa.fa-pinterest
                .col-md-8
                  .contact-area-right
                    form.comments-form.contact-form(action='')
                      .form-group
                        input.form-control(type='text', placeholder='Your Name')
                      .form-group
                        input.form-control(type='email', placeholder='Email')
                      .form-group
                        input.form-control(type='text', placeholder='Subject')
                      .form-group
                        textarea.form-control(placeholder='Comment', rows='3')
                      button.comment-btn Submit Comment
    // End contact section
    // Start google map
    section#google-map
      iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3902.3714257064535!2d-86.7550931378034!3d34.66757706940219!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8862656f8475892d%3A0xf3b1aee5313c9d4d!2sHuntsville%2C+AL+35813%2C+USA!5e0!3m2!1sen!2sbd!4v1445253385137', width='100%', height='450', frameborder='0', style='border:0', allowfullscreen='')
    // End google map
    // Start subscribe us
    section#subscribe
      .subscribe-overlay
        .container
          .row
            .col-md-12
              .subscribe-area
                h2 Subscribe Newsletter
                form.subscrib-form(action='')
                  input(type='text', placeholder='Enter Your E-mail..')
                  button.subscribe-btn(type='submit') Submit
    // End subscribe us
    // Start footer
    footer#footer
      .container
        .row
          .col-md-6.col-sm-6
            .footer-left
              p
                | Designed by 
                a(href='http://www.markups.io/') MarkUps.io
          .col-md-6.col-sm-6
            .footer-right
              a(href='index')
                i.fa.fa-facebook
              a(href='#')
                i.fa.fa-twitter
              a(href='#')
                i.fa.fa-google-plus
              a(href='#')
                i.fa.fa-linkedin
              a(href='#')
                i.fa.fa-pinterest
    // End footer
    // jQuery library
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
    // Include all compiled plugins (below), or include individual files as needed
    // Bootstrap
    script(src='js/bootstrap.js')
    // Slick Slider
    script(type='text/javascript', src='js/slick.js')
    // mixit slider
    script(type='text/javascript', src='js/jquery.mixitup.js')
    // Add fancyBox
    script(type='text/javascript', src='js/jquery.fancybox.pack.js')
    // counter
    script(src='js/waypoints.js')
    script(src='js/jquery.counterup.js')
    // Wow animation
    script(type='text/javascript', src='js/wow.js')
    // progress bar
    script(type='text/javascript', src='js/bootstrap-progressbar.js')
    // Custom js
    script(type='text/javascript', src='js/custom.js')
