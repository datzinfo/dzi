doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
    title DatzInfo : Service
    // Favicon
    link(rel='shortcut icon', type='image/icon', href='images/favicon.ico')
    // Font Awesome
    link(href='css/font-awesome.css', rel='stylesheet')
    // Bootstrap
    link(href='css/bootstrap.css', rel='stylesheet')
    // Slick slider
    link(rel='stylesheet', type='text/css', href='css/slick.css')
    // Fancybox slider
    link(rel='stylesheet', href='css/jquery.fancybox.css', type='text/css', media='screen')
    // Animate css
    link(rel='stylesheet', type='text/css', href='css/animate.css')
    // Bootstrap progressbar
    link(rel='stylesheet', type='text/css', href='css/bootstrap-progressbar-3.3.4.css')
    // Theme color
    link#switcher(href='css/theme-color/default-theme.css', rel='stylesheet')
    // Main Style
    link(href='css/style.css', rel='stylesheet')
    // Fonts
    // Open Sans for body font
    link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet', type='text/css')
    // Lato for Title
    link(href='https://fonts.googleapis.com/css?family=Lato', rel='stylesheet', type='text/css')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body
    // BEGAIN PRELOADER
    #preloader
      #status Â 
    // END PRELOADER
    // SCROLL TOP BUTTON
    a.scrollToTop(href='#')
      i.fa.fa-angle-up
    // END SCROLL TOP BUTTON
    // BEGIN MENU
    section#menu-area
      nav.navbar.navbar-default(role='navigation')
        .container
          .navbar-header
            // FOR MOBILE VIEW COLLAPSED BUTTON
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            // LOGO
            // TEXT BASED LOGO
            a.navbar-brand(href='index.html') DatzInfo
            // IMG BASED LOGO
            // <a class="navbar-brand" href="index.html"><img src="images/logo.png" alt="logo"></a>
          #navbar.navbar-collapse.collapse
            ul#top-menu.nav.navbar-nav.navbar-right.main-nav
              li
                a(href='index.html') Home
              li
                a(href='aboutUs.html') About Us
              li.active
                a(href='service.html') Services
              li
                a(href='blog.html') Blog
              li
                a(href='contact.html') Contact
          // /.nav-collapse
    // END MENU
    // Start single page header
    section#single-page-header
      .overlay
        .container
          .row
            .col-md-6.col-sm-6.col-xs-12
              .single-page-header-left
                h2 #{ messages.services_banner_title }
                p #{ messages.services_banner_desc }
    // End single page header
    // Start Service
    section#service
      .container
        .row
          .col-md-12
            .title-area
              h2.title #{ messages.services_banner_title }
              span.line
              p #{ messages.services_banner_desc }
          .col-md-12
            .service-content
              .row
                // Start single service
                .col-md-4.col-sm-6
                  .single-service.wow.zoomIn
                    i.fa.fa-desktop.service-icon
                    h4.service-title #{ messages.services_box1_title }
                    p #{ messages.services_box1_desc }
                // End single service
                // Start single service
                .col-md-4.col-sm-6
                  .single-service.wow.zoomIn
                    i.fa.fa-paw.service-icon
                    h4.service-title #{ messages.services_box2_title }
                    p #{ messages.services_box2_desc }
                // End single service
                // Start single service
                .col-md-4.col-sm-6
                  .single-service.wow.zoomIn
                    i.fa.fa-magic.service-icon
                    h4.service-title #{ messages.services_box3_title }
                    p #{ messages.services_box3_desc }
                // End single service
                // Start single service
                .col-md-4.col-sm-6
                  .single-service.wow.zoomIn
                    i.fa.fa-shopping-cart.service-icon
                    h4.service-title #{ messages.services_box4_title }
                    p #{ messages.services_box4_desc }
                // End single service
                // Start single service
                .col-md-4.col-sm-6
                  .single-service.wow.zoomIn
                    i.fa.fa-mobile.service-icon
                    h4.service-title #{ messages.services_box5_title }
                    p #{ messages.services_box5_desc }
                // End single service
                // Start single service
                .col-md-4.col-sm-6
                  .single-service.wow.zoomIn
                    i.fa.fa-rocket.service-icon
                    h4.service-title #{ messages.services_box6_title }
                    p #{ messages.services_box6_desc }
                // End single service
    // End Service
    // Start subscribe us
    section#subscribe
      .subscribe-overlay
        .container
          .row
            .col-md-12
              .subscribe-area
                h2.wow.fadeInUp Subscribe Newsletter
                form.subscrib-form.wow.fadeInUp(action='', data-wow-duration='0.5s', data-wow-delay='0.5s')
                  input(type='text', placeholder='Enter Your E-mail..')
                  button.subscribe-btn(type='submit') Submit
    // End subscribe us
    // Start footer
    footer#footer
      .container
        .row
          .col-md-6.col-sm-6
            .footer-left
              p
                | Designed by 
                a(href='http://www.markups.io/') MarkUps.io
          .col-md-6.col-sm-6
            .footer-right
              a(href='index.html')
                i.fa.fa-facebook
              a(href='#')
                i.fa.fa-twitter
              a(href='#')
                i.fa.fa-google-plus
              a(href='#')
                i.fa.fa-linkedin
              a(href='#')
                i.fa.fa-pinterest
    // End footer
    // jQuery library
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
    // Include all compiled plugins (below), or include individual files as needed
    // Bootstrap
    script(src='js/bootstrap.js')
    // Slick Slider
    script(type='text/javascript', src='js/slick.js')
    // mixit slider
    script(type='text/javascript', src='js/jquery.mixitup.js')
    // Add fancyBox
    script(type='text/javascript', src='js/jquery.fancybox.pack.js')
    // counter
    script(src='js/waypoints.js')
    script(src='js/jquery.counterup.js')
    // Wow animation
    script(type='text/javascript', src='js/wow.js')
    // progress bar
    script(type='text/javascript', src='js/bootstrap-progressbar.js')
    // Custom js
    script(type='text/javascript', src='js/custom.js')
